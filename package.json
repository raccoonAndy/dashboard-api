{
  "name": "dashboard_api",
  "version": "1.0.0",
  "description": "API for Education project",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "build": "rm -Rf ./dist && tsc",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "clinic": "rm -Rf ./.clinic && clinic doctor --on-port 'autocannon -m POST localhost:8000/users/register' -- node dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "4.17.2",
    "inversify": "6.0.1",
    "reflect-metadata": "0.1.13",
    "tslog": "3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "4.17.13",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  }
}
